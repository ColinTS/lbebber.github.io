article.Work
  div.Work-text
    h3.Work-title
      | #{title}
      .Work-subtitle #{subtitle}
    p.Work-description #{description}
    for item in links
      a.Work-link(href=item.link)
        | #{item.label}
  figure.Work-image.js-Lazyload.js-Lazyload--hidden(data-image=image,data-hires='true',role='presentation')


